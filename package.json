{
  "name": "multikart",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run multikart:serve-ssr",
    "build:ssr": "ng build --prod && ng run multikart:server:production",
    "build-staging:ssr": "ng build --configuration=staging && ng run multikart:server:staging",
    "build-prod:ssr": "ng build --configuration=production && ng run multikart:server:production",
    "serve:ssr": "node dist/multikart/server/main.js",
    "prerender": "ng run multikart:prerender",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "format:fix": "pretty-quick --staged",
    "precommit": "run-s format:fix lint",
    "precheck": "yarn format:fix && yarn lint",
    "commit": "yarn precheck && git add . && git-cz"
  },
  "private": true,
